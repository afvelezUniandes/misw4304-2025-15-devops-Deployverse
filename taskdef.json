{
  "family": "blacklist-entrega3-task",
  "executionRoleArn": "arn:aws:iam::014487871519:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "blacklist_entrega3",
      "image": "<IMAGE1_NAME>",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 5000,
          "hostPort": 5000
        }
      ],
      "environment": [
        { "name": "NEW_RELIC_CONFIG_FILE", "value": "/app/newrelic.ini" },
        {
          "name": "NEW_RELIC_LICENSE_KEY",
          "value": "3bf3db90f3a1a9d6901901d1dc054553FFFFNRAL"
        },
        { "name": "NEW_RELIC_APP_NAME", "value": "blacklist_app" }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/blacklist-entrega3-task",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "command": [
        "newrelic-admin",
        "run-program",
        "gunicorn",
        "--config",
        "gunicorn.conf.py",
        "application:application"
      ]
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "3072"
}
